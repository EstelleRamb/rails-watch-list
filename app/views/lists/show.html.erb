

    <div class="films">
    <% @list.bookmarks.each do |bookmark| %>
      <div class="card p-2 mt-4 ">
      <img src="<%= bookmark.movie.poster_url %>" class="films-category" alt="...">
      <div class="p-3">
       <h5><%= bookmark.movie.title %></h5>
       <p><%= bookmark.movie.overview %></p>
       <p>Rating <%= bookmark.movie.rating %></p>
        <span><%= bookmark.comment %></span>
          <div>
          <a><%= link_to 'Supprimer', bookmark_path(bookmark),method: :delete, class:"btn btn-dark"%>
           </a>
          </div>
        </div>
      </div>

      <% end %>
    </div>

  <a><%= link_to 'Add movie', new_list_bookmark_path(@list), class:"btn btn-dark"%></a>
   <a><%= link_to "Back to lists", root_path, class:"btn btn-dark"%></a>
